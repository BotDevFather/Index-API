{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/store",
      "methods": ["POST"],
      "dest": "/server.js"
    },
    {
      "src": "/chat",
      "methods": ["GET"],
      "dest": "/server.js"
    }
  ]
}
